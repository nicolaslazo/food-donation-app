{{#partial "titulo"}}Reportes - Viandas Donation{{/partial}}

{{#partial "styles"}}
    <link rel="stylesheet" href="/public/css/verReportes/styles.css">
{{/partial}}

{{#partial "scripts"}}
    <script src="/public/js/sticky_nav_bar.js" defer></script>
    <script src="/public/js/verReportes/script.js" defer></script>
{{/partial}}

{{#partial "contenido"}}
<main>
    <div class="accordion-container">
        {{#each reportes}}
        <div class="accordion">
            <div class="accordion-title">{{this.titulo}}</div>
            <div class="accordion-content">
                <embed src="{{this.rutaArchivo}}" type="application/pdf" width="100%" height="400px" />
            </div>
        </div>
        {{/each}}
    </div>
</main>
{{/partial}}

{{#partial "contenido-footer"}}
    <p>Copyright Â© 2024 Viandas Donation. Todos los derechos reservados.</p>
{{/partial}}

{{> templates/base}}
